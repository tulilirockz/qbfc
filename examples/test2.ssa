export
function w $main() {
@start.1
	%.1 =l alloc4 30000
	%.2 =l alloc4 4
@body.2
	storew 0, %.2
	%.3 =w loadw %.2
	%.4 =l extsw %.3
	%.5 =l mul %.4, 1
	%.6 =l add %.1, %.5
	%.7 =w loadsb %.6
	%.8 =w extsb %.7
	%.9 =w add %.8, 1
	storeb %.9, %.6
	%.10 =w loadw %.2
	%.11 =l extsw %.10
	%.12 =l mul %.11, 1
	%.13 =l add %.1, %.12
	%.14 =w loadsb %.13
	%.15 =w extsb %.14
	%.16 =w sub %.15, 1
	storeb %.16, %.13
	ret 0
}
